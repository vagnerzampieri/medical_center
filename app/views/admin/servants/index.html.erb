        <%= render :partial => 'admin/shared/subnav_servants'%>
        
        <article class="article">
          <h2><%= I18n.t :all_servants %></h2>
          
          <div class="line"></div> 
          
          <div class="articleBody clear">  
		        <table>
		        	<thead>
		        	  <th><%= I18n.t :type %></th>
		        		<th><%= I18n.t :name %></th>
		        		<th><%= I18n.t :email %></th>
		        		<th><%= I18n.t :telephone %></th>
		        		<th><%= I18n.t :city %></th>
		        		<th><%= I18n.t :state %></th>
		        	</thead>
		        	<% @servants.each do |servant| %>
		        	<tr>
		        	  <td><%= I18n.t "#{servant.type.downcase}" %></td>
		        		<td><%= servant.name %></td>
		        		<td><%= servant.email %></td>
		        		<td><%= servant.area_code %> - <%= servant.telephone %></td>
		        		<td><%= servant.city %></td>
		        		<td><%= servant.state.name unless servant.state.nil? %></td>
		        	</tr>
		        	<% end %>
		        </table>
          </div>
        </article>

